      -
        code : |1+
         // Procedure called before each appointment drawing in an Organizer, Scheduler or TimeLine control
         // This code is re-run for EACH appointment display
         PROCEDURE IW_WinDevViewAPT( EventOrAPT )
         //EventOrAPT: Appointment or Event to draw
         
         // => SourceToScreen
         STC_Details 						= EventOrAPT.Content
         STC_APTTitle 					= EventOrAPT.Title
         STC_BackgroundAPTTitle..BrushColor	= EventOrAPT.BrushColor
         IMG_Image1..Value 				= EventOrAPT.Image
         IMG_Important..Visible			= (EventOrAPT.Importance <> 0)
         IMG_REPETITION..Visible			= EventOrAPT.WithRepetition
         
         
         // if the ATP is small and if we have a logo
         IF MySelf..Height <= 2*STC_BackgroundAPTTitle..Height _AND_  EventOrAPT.Image<>"" THEN
         	// place the caption beside the title
         	IMG_Image1..Y = STC_APTTitle..Y
         	STC_APTTitle..X = IMG_Image1..X + IMG_Image1..Width
         END
      -
        type : 15
