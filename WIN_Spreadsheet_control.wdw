  type_code : 1
  p_codes :
   -
     code : |1+
      
      
   -
     code : |1+
      xBorder is a Border // To add a border into the spreadsheet
      
      // Add data into the spreadsheet
      SpreadsheetAddData(PSHEET_Spreadsheet,1,1,"Reference")
      SpreadsheetAddData(PSHEET_Spreadsheet,2,1,"Product caption")
      SpreadsheetAddData(PSHEET_Spreadsheet,3,1,"Description")
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"A1","C1")
      SpreadsheetStyleSelection(PSHEET_Spreadsheet,psheetBold,True)
      
      // Add borders
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"A1","C1")
      xBorder..Thickness = 2
      xBorder..Color = Black
      
      
      SpreadsheetBorderSelection(PSHEET_Spreadsheet, xBorder)
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"A2","C9")
      xBorder..Thickness = 0.5
      xBorder..Color = Black
      SpreadsheetBorderSelection(PSHEET_Spreadsheet, xBorder)
      
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"A2","A9")
      xBorder..Thickness = 0.5
      xBorder..Color = Black
      SpreadsheetBorderSelection(PSHEET_Spreadsheet, xBorder)
      
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"B2","B9")
      xBorder..Thickness = 0.5
      xBorder..Color = Black
      SpreadsheetBorderSelection(PSHEET_Spreadsheet, xBorder)
      
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"C2","C9")
      xBorder..Thickness = 0.5
      xBorder..Color = Black
      SpreadsheetBorderSelection(PSHEET_Spreadsheet, xBorder)
      
      
      // load products in the control
      //open the file to read it
      HOpen(PRODUCT)
      //position on the first file record
      HReadFirst(PRODUCT)
      FOR i = 1 TO 8
      	SpreadsheetAddData(PSHEET_Spreadsheet,1,i+1,PRODUCT.Reference)
      	SpreadsheetAddData(PSHEET_Spreadsheet,2,i+1,PRODUCT.ProdDesc)
      	SpreadsheetAddData(PSHEET_Spreadsheet,3,i+1,PRODUCT.Description)
      	//continue to browse the file to fill the control
      	HReadNext(PRODUCT)
      END
      //close the file that was read
      HClose(PRODUCT)
      
      
      SpreadsheetStyleSelection(PSHEET_Spreadsheet,psheetFontSize,12)
      
      SpreadsheetSelectMinus(PSHEET_Spreadsheet)
      // Increase the size of references
      SpreadsheetSelectPlus(PSHEET_Spreadsheet,"A2","A9")
      SpreadsheetStyleSelection(PSHEET_Spreadsheet,psheetFontSize,16)
